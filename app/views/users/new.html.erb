<h1>Create a New User!</h1>

<%= render 'shared/errors', obj: @user %>

<%= form_for @user do |f| %>
<p>
  <%= f.label :username %><br/>
  <%= f.text_field :username %>
</p>
<p>
  <%= f.label :password %><br/>
  <%= f.text_field :password %>
</p>
<p>
  <%= f.submit %>
</p>
<% end %>
